{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {   
      "src": "/v1/api-docs",
      "dest": "/docs/api-docs.json",
      "headers": { "Content-Type": "application/json" }
    },
    {
      "src": "/v1/api-docs",
      "dest": "/docs/api-docs.yaml",
      "headers": { "Content-Type": "application/yaml" }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
